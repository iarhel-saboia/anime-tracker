<h1>Users#index</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Role</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.role %></td>
      </tr>
    <% end %>
  </tbody>
</table>
